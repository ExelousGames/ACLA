{
  "name": "acla_front",
  "version": "0.1.0",
  "private": true,
  "author": "Captain Electron",
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/themes": "^3.2.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "axios": "^1.9.0",
    "konva": "^9.3.20",
    "python-shell": "^5.0.0",
    "radix-ui": "^1.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-konva": "^19.0.4",
    "react-konva-utils": "^1.1.0",
    "react-router-dom": "^7.6.1",
    "react-scripts": "5.0.1",
    "use-image": "^1.1.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "setup-speech": "node setup-speech.js",
    "start:electron": "npm run setup-speech && concurrently \"env-cmd -f ../.dev.env react-scripts start\" \"wait-on http://localhost:3000 && electron .\"",
    "build:electron": "npm run setup-speech && env-cmd -f ../.prod.env react-scripts build && npx electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "electron": "^36.4.0",
    "electron-builder": "^26.0.12",
    "electron-is-dev": "^3.0.1",
    "env-cmd": "^10.1.0",
    "typescript": "^4.1.6",
    "wait-on": "^8.0.3"
  },
  "main": "public/electron.js",
  "build": {
    "appId": "com.example.myapp",
    "productName": "My Electron App",
    "win": {
      "target": "nsis"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "homepage": "./"
}